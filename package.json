{
  "name": "tanstack-start-db-electric-starter",
  "private": true,
  "version": "0.1.0",
  "type": "module",
  "engines": {
    "node": ">=20.19.0 || >=22.12.0"
  },
  "scripts": {
    "dev": "bunx --bun vite dev",
    "backend:up": "docker compose up -d",
    "backend:down": "docker compose down",
    "backend:clear": "docker compose down -v",
    "start": "bun .output/server/index.mjs",
    "build": "bunx --bun vite build",
    "migrate": "bunx drizzle-kit migrate",
    "migrate:generate": "bunx drizzle-kit generate",
    "psql": "dotenvx run -- sh -c 'psql \"$DATABASE_URL\"'",
    "serve": "bunx --bun vite preview",
    "test": "bunx vitest run",
    "lint:check": "bunx eslint .",
    "lint": "bunx eslint . --fix",
    "format": "bunx prettier --write .",
    "format:check": "bunx prettier --check ."
  },
  "dependencies": {
    "@electric-sql/client": "^1.2.0",
    "@tailwindcss/vite": "^4.0.6",
    "@tanstack/electric-db-collection": "^0.2.8",
    "@tanstack/react-db": "^0.1.52",
    "@tanstack/react-router": "^1.139.7",
    "@tanstack/react-router-devtools": "^1.139.7",
    "@tanstack/react-start": "^1.139.9",
    "@tanstack/router-plugin": "^1.139.7",
    "@orpc/client": "^1.7.11",
    "@orpc/server": "^1.7.11",
    "arktype": "^2.1.27",
    "better-auth": "^1.4.3",
    "drizzle-arktype": "^0.1.3",
    "drizzle-orm": "^0.39.0",
    "lucide-react": "^0.544.0",
    "nitro": "npm:nitro-nightly@3.0.1-20251209-203952-cd9c2915",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "tailwindcss": "^4.1.13"
  },
  "devDependencies": {
    "@dotenvx/dotenvx": "^1.51.2",
    "@eslint/compat": "^1.3.1",
    "@eslint/js": "^9.32.0",
    "@tanstack/devtools-vite": "^0.3.11",
    "@testing-library/dom": "^10.4.1",
    "@testing-library/react": "^16.2.0",
    "@types/bun": "^1.3.5",
    "@types/node": "^22.10.2",
    "@types/react": "^19.2.0",
    "@types/react-dom": "^19.2.0",
    "@typescript-eslint/eslint-plugin": "^8.46.0",
    "@typescript-eslint/parser": "^8.46.0",
    "@vitejs/plugin-react": "^5.0.4",
    "drizzle-kit": "^0.31.4",
    "eslint": "^9.32.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.3",
    "eslint-plugin-react": "^7.37.5",
    "jsdom": "^27.0.0",
    "open-cli": "^8.0.0",
    "prettier": "^3.6.2",
    "sst": "3.13.2",
    "tsx": "^4.20.3",
    "typescript": "^5.7.2",
    "vite": "^7.1.7",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "^4.0.15",
    "web-vitals": "^5.1.0"
  }
